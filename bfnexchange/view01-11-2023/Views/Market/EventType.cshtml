	function AssignDataToSoccerCard(matchstr) {

		if (matchstr == "") {
			$("#matchscores").hide();
			return;
		}

		var status = $("#marketbookstatus" + $("#lastloadedmarket").val().toString().replace(".", "")).html();
		var eventID = $("#eventidfor" + $("#lastloadedmarket").val().toString().replace(".", "")).val();
		var scorecard = JSON.parse(matchstr);

		if (scorecard.length == 0 || scorecard.length == 1 || status == "CLOSED" || status == "GOING LIVE" || status == "In-Play" || status == "in-play" || status == "in play") {
			$("#matchscores").hide();
            return;
		}

		else {
			//debugger
            if (scorecard.timeElapsed >= 45) {
                $("#awaytimeline").width(100 + "%").css('background-color', 'green');
            }
            else {
                var goaltime = scorecard.timeElapsed * 2;
                goaltime = goaltime * 0.1;
                $("#awaytimeline").width(((scorecard.timeElapsed * 2) + goaltime) + "%").css('background-color', 'green');
            }

